{
  "env": {
    "development": {
      "plugins": [
        [
          "wrap-in-js",
          {
            "extensions": ["css$", "scss$"]
          }
        ]
      ],
      "presets": [
        "next/babel"
      ],
      "ignore": [],
    },
    "production": {
      "plugins": [
        [
          "wrap-in-js",
          {
            "extensions": ["css$", "scss$"]
          }
        ]
      ],
      "presets": [
        "next/babel"
      ],
      "ignore": [],
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  }
}
